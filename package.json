{
  "name": "prettier-config-vladpuz",
  "version": "2.0.0",
  "description": "My Prettier config",
  "keywords": [
    "vladpuz",
    "prettier",
    "prettierconfig"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladpuz/prettier-config-vladpuz.git"
  },
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check": "npm run typecheck && npm run lint && npm run format",
    "clean": "rimraf build",
    "format": "prettier --check .",
    "lint": "eslint .",
    "prepack": "npm run clean && npm run build && npm run check",
    "prepare": "husky",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "prettier-plugin-sh": "^0.18.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.3",
    "@tsconfig/node-ts": "^23.6.2",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-vladpuz": "^2.5.1",
    "husky": "^9.1.7",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
